<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
         <title>testing</title>


     </head>
    <body>
       <div >
          <img class="slide" src="/imagenes/diagnostico-computadora-reparacion3.jpg">
          <img class="slide" src="/imagenes/diagnostico-computadora-reparacion5.jpg">
          <img class="slide" src="/imagenes/diagnostico-computadora-reparacion6.jpg">
          <a class="w3-btn-floating" onclick="plusDivs(-1)">&#10094;</a>
          <a class="w3-btn-floating" onclick="plusDivs(+1)">&#10095;</a>
          <script language="javascript" type="text/javascript">
           -
          var slideIndex = 1;
          showDivs(slideIndex);

          function plusDivs(n) {
              showDivs(slideIndex += n);
          }

          function showDivs(n) {
              var i;
              var x = document.getElementsByClassName("slide");
              if (n > x.length) {slideIndex = 1}
              if (n < 1) {slideIndex = x.length} ;
              for (i = 0; i < x.length; i++) {
                  x[i].style.display = "none";
              }
              x[slideIndex-1].style.display = "block";
          }
          </script>

       </div>
    </body>
</html>
